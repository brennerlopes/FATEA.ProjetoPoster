@model IEnumerable<FATEA.ProjetoPoster.Web.ViewModels.Poster.PosterIndexViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Enviar poster", "Create", null, new { @class = "btn btn-default" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomeEvento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Autores)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PalavraChave)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resumo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Modalidade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomeCurso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nota)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AvaliadoPor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomeArquivo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
         <td>
            @Html.DisplayFor(modelItem => item.NomeEvento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Autores)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PalavraChave)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Resumo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Modalidade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomeCurso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nota)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AvaliadoPor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomeArquivo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
